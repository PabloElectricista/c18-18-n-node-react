{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": "warn",
    "semi": "error",
    " linebreak-style": 0,
  
    "curly": "off",
    "no-console": "off"

  }
}
